{
	"name": "nest-server-tools",
	"displayName": "Nest-Server-Tools",
	"publisher": "chogath",
	"description": "🦁 帮助 nest-server 开发者在 vscode 中快速创建模板/目录/文件",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"icon": "public/images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/sophons-space/nest-server-tools"
	},
	"bugs": {
		"url": "https://github.com/sophons-space/nest-server-tools/issues",
		"email": "chogath@163.com"
	},
	"activationEvents": [
		"onCommand:extension.BO",
		"onCommand:extension.DTO",
		"onCommand:extension.Controller",
		"onCommand:extension.Extends",
		"onCommand:extension.Filter",
		"onCommand:extension.Guard",
		"onCommand:extension.Interceptor",
		"onCommand:extension.Module.File",
		"onCommand:extension.Sequelize.File",
		"onCommand:extension.Modules",
		"onCommand:extension.Pipe",
		"onCommand:extension.Provider",
		"onCommand:extension.Service"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "nest-server-tools"
		},
		"commands": [
			{
				"command": "extension.Modules",
				"title": "🏗 Nest-S: Module"
			},
			{
				"command": "extension.Extends",
				"title": "🏗 Nest-S: Extend"
			},
			{
				"command": "extension.Module.File",
				"title": "📄 Nest-S: module"
			},
			{
				"command": "extension.Sequelize.File",
				"title": "📄 Nest-S: sequelize"
			},
			{
				"command": "extension.BO",
				"title": "📦 Nest-S: BO"
			},
			{
				"command": "extension.DTO",
				"title": "📦 Nest-S: DTO"
			},
			{
				"command": "extension.Controller",
				"title": "📦 Nest-S: Controller"
			},
			{
				"command": "extension.Service",
				"title": "📦 Nest-S: Service"
			},
			{
				"command": "extension.Provider",
				"title": "📦 Nest-S: Provider"
			},
			{
				"command": "extension.Filter",
				"title": "📦 Nest-S: Filter"
			},
			{
				"command": "extension.Guard",
				"title": "📦 Nest-S: Guard"
			},
			{
				"command": "extension.Interceptor",
				"title": "📦 Nest-S: Interceptor"
			},
			{
				"command": "extension.Pipe",
				"title": "📦 Nest-S: Pipe"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.Modules",
					"when": "explorerResourceIsFolder && resourceFilename == modules",
					"group": "navigation"
				},
				{
					"command": "extension.Extends",
					"when": "explorerResourceIsFolder && resourceFilename == extends",
					"group": "navigation"
				},
				{
					"command": "extension.BO",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.DTO",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Controller",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Service",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Filter",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Interceptor",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Pipe",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Provider",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Guard",
					"when": "explorerResourceIsFolder",
					"group": "9_cutcopypaste"
				},
				{
					"command": "extension.Module.File",
					"when": "explorerResourceIsFolder",
					"group": "7_modification"
				},
				{
					"command": "extension.Sequelize.File",
					"when": "explorerResourceIsFolder",
					"group": "7_modification"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"package": "cp -r src/templates ./out && vsce package"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/fs-extra": "^9.0.6",
		"@types/mustache": "^4.1.0",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"fs-extra": "^9.0.1",
		"mustache": "^4.1.0"
	}
}
